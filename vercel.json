{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://api.timbu.cloud/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
